<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record forcecreate="True" id="ir_cron_create_quotations_action" model="ir.cron">
            <field name="name">Parcel Pro : Create Quotation</field>
            <field name="model_id" ref="model_sale_order"/>
            <field name="state">code</field>
            <field name="code">model.process_create_quotation()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>

         <record forcecreate="True" id="ir_cron_create_shipments_action" model="ir.cron">
            <field name="name">Parcel Pro : Create Shipment</field>
            <field name="model_id" ref="model_stock_picking"/>
            <field name="state">code</field>
            <field name="code">model.process_create_shipment()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field eval="False" name="doall"/>
        </record>



    </data>
</odoo>