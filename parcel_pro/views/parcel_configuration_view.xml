<odoo>
    <data>

        <record id="parcel_view_tree" model="ir.ui.view">
            <field name="name">Parcel Tree</field>
            <field name="model">parcel.configuration</field>
            <field name="arch" type="xml">
                <tree string="Parcel Configuration" create='false' delete='false'>
                    <field name="user_name"/>
                    <field name="password"/>
                </tree>
            </field>
        </record>

        <record id="parcel_config_view_form" model="ir.ui.view">
            <field name="name">Parcel Configuration Form</field>
            <field name="model">parcel.configuration</field>
            <field name="arch" type="xml">
                <form string="Parcel Configuration" create='false' delete='false'>
                    <sheet>
                        <group col="4">
                            <field name="name"/>
                            <field name="user_name"/>
                            <field name="password"/>
                            <field name="api_key"/>
                            <field name="session" readonly="1"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="action_parcel_configuration" model="ir.actions.act_window">
            <field name="name">Parcel Configuration</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">parcel.configuration</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="main_menu_parcel" name="Parcel" sequence="1"/>
        <menuitem id="main_menu_parcel_config" name="Configuration" sequence="5" parent="main_menu_parcel"/>
        <menuitem id="submenu_parcel_pro_config" name="Configuration" parent="main_menu_parcel_config"  action="action_parcel_configuration" sequence="2"/>

    </data>
</odoo>